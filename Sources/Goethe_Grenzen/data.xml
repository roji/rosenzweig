                           <div type="text" xml:id="tg193.3">

                              <div type="h4">
                                 <head type="h4" xml:id="tg193.3.1">Grenzen der Menschheit</head>

                                 <lg>
                                    <l xml:id="tg193.3.2">Wenn der uralte</l>
                                    <l xml:id="tg193.3.3">Heilige Vater</l>
                                    <l xml:id="tg193.3.4">Mit gelassener Hand</l>
                                    <l xml:id="tg193.3.5">Aus rollenden Wolken</l>
                                    <l xml:id="tg193.3.6">Segnende Blitze</l>
                                    <l xml:id="tg193.3.7">Über die Erde sät,</l>
                                    <l xml:id="tg193.3.8">Küß ich den letzten</l>
                                    <l xml:id="tg193.3.9">Saum seines Kleides,</l>
                                    <l xml:id="tg193.3.10">Kindliche Schauer</l>
                                    <l xml:id="tg193.3.11">Treu in der Brust.</l>
                                 </lg>
                                 <lg>
                                    <lb xml:id="tg193.3.12"/>
                                    <l xml:id="tg193.3.13">Denn mit Göttern</l>
                                    <l xml:id="tg193.3.14">Soll sich nicht messen</l>
                                    <l xml:id="tg193.3.15">Irgendein Mensch</l>
                                    <l xml:id="tg193.3.16">Hebt er sich aufwärts</l>
                                    <l xml:id="tg193.3.17">Und berührt</l>
                                    <l xml:id="tg193.3.18">Mit dem Scheitel die Sterne,</l>
                                    <l xml:id="tg193.3.19">Nirgends haften dann</l>
                                    <l xml:id="tg193.3.20">Die unsichern Sohlen,</l>
                                    <l xml:id="tg193.3.21">Und mit ihm spielen</l>
                                    <l xml:id="tg193.3.22">Wolken und Winde.</l>
                                 </lg>
                                 <lg>
                                    <lb xml:id="tg193.3.23"/>
                                    <l xml:id="tg193.3.24">Steht er mit festen,</l>
                                    <l xml:id="tg193.3.25">Markigen Knochen</l>
                                    <l xml:id="tg193.3.26">Auf der wohlgegründeten,</l>
                                    <l xml:id="tg193.3.27">Dauernden Erde,</l>
                                    <l xml:id="tg193.3.28">Reicht er nicht auf,</l>
                                    <l xml:id="tg193.3.29">Nur mit der Eiche</l>
                                    <l xml:id="tg193.3.30">Oder der Rebe</l>
                                    <l xml:id="tg193.3.31">Sich zu vergleichen.</l>
                                 </lg>
                                 <lg>
                                    <lb xml:id="tg193.3.32"/>
                                    <l xml:id="tg193.3.33">Was unterscheidet</l>
                                    <l xml:id="tg193.3.34">Götter von Menschen?</l>
                                    <l xml:id="tg193.3.35">Daß viele Wellen</l>
                                 </lg>
                                 <lg>
                                    <lb xml:id="tg193.3.36"/>
                                    <pb n="330" xml:id="tg193.3.37"/>
                                    <l xml:id="tg193.3.38">Vor jenen wandeln,</l>
                                    <l xml:id="tg193.3.39">Ein ewiger Strom:</l>
                                    <l xml:id="tg193.3.40">Uns hebt die Welle,</l>
                                    <l xml:id="tg193.3.41">Verschlingt die Welle,</l>
                                    <l xml:id="tg193.3.42">Und wir versinken.</l>
                                 </lg>
                                 <lg>
                                    <lb xml:id="tg193.3.43"/>
                                    <l xml:id="tg193.3.44">Ein kleiner Ring</l>
                                    <l xml:id="tg193.3.45">Begrenzt unser Leben,</l>
                                    <l xml:id="tg193.3.46">Und viele Geschlechter</l>
                                    <l xml:id="tg193.3.47">Reihen sich dauernd</l>
                                    <l xml:id="tg193.3.48">An ihres Daseins</l>
                                    <l xml:id="tg193.3.49">Unendliche Kette.</l>
                                 </lg>
                                 <lb xml:id="tg193.3.50"/>
                              </div>
